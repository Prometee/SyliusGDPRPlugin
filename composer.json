{
    "name": "synolia/sylius-gdpr-plugin",
    "type": "sylius-plugin",
    "description": "Synolia sylius GDPR plugin.",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "gdpr",
        "rgpd",
        "synolia"
    ],
    "license": "EUPL-1.2",
    "require": {
        "php": "^7.4 || ^8.0",
        "sylius/sylius": "^1.9"
    },
    "require-dev": {
        "j13k/yaml-lint": "1.1.4",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "phpmd/phpmd": "2.12.0",
        "phpro/grumphp": "1.13.0",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "1.6.3",
        "phpstan/phpstan-doctrine": "1.3.3",
        "phpstan/phpstan-strict-rules": "1.2.1",
        "phpstan/phpstan-webmozart-assert": "1.1.2",
        "phpunit/phpunit": "9.5.21",
        "povils/phpmnd": "2.5.0",
        "seld/jsonlint": "1.9.0",
        "sylius-labs/coding-standard": "4.1.1",
        "symplify/easy-coding-standard": "9.4.70",
        "symfony/browser-kit": "5.4.3",
        "symfony/debug-bundle": "5.4.3",
        "symfony/dotenv": "5.4.5",
        "symfony/intl": "5.4.10",
        "symfony/web-profiler-bundle": "5.4.10",
        "thecodingmachine/phpstan-strict-rules": "1.0.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "^4.3 || ^5.1"
        }
    },
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusGDPRPlugin\\": "src/",
            "Tests\\Synolia\\SyliusGDPRPlugin\\": "tests/"
        }
    },
    "scripts": {
        "fix-ecs": "ecs check -c ruleset/ecs.php --fix --ansi --clear-cache .",
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
